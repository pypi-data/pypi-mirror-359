<odoo>
    <record
        id="view_order_product_search_inherit_partner_code_program_territory"
        model="ir.ui.view"
    >
        <field
            name="name"
        >view.order.product.search.inherit.partner.code.program.territory</field>
        <field name="model">sale.report</field>
        <field name="inherit_id" ref="sale.view_order_product_search" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='partner_id']" position="after">
                <field name="code_program_territory_id" />
            </xpath>
            <xpath expr="//group/filter[@name='Customer']" position="after">
                <filter
                    string="Code Program Territory"
                    name="code_program_territory"
                    domain="[]"
                    context="{'group_by': 'code_program_territory_id'}"
                />
            </xpath>
        </field>
    </record>
</odoo>
